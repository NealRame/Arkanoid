  "private": true,
  "dependencies": {
    "babel-preset-es2015": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "del": "^2.2.2",
    "eslint": "^3.7.1",
    "gulp": "^3.9.1",
    "gulp-cli": "^1.2.2",
    "gulp-if": "^2.0.1",
    "gulp-livereload": "^3.8.1",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.0.0"
