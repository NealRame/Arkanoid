  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": [],
    "parserOptions": {
      "ecmaFeatures": {},
      "sourceType": "module"
    },
    "rules": {
      "camelcase": 0,
      "curly": [
        1,
        "multi-line"
      ],
      "strict": [
        2,
        "global"
      ],
      "key-spacing": 0,
      "new-cap": [
        1,
        {
          "newIsCap": true,
          "capIsNew": false
        }
      ],
      "no-multi-spaces": 0,
      "no-var": "error",
      "quotes": [
        1,
        "single"
      ],
      "space-infix-ops": 0
    }
  }
